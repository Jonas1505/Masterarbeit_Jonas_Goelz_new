@article{waltherMethodologyClassificationCharacterisation2022,
  title = {A {{Methodology}} for the {{Classification}} and {{Characterisation}} of {{Industrial Demand-Side Integration Measures}}},
  author = {Walther, Jessica and Dietrich, Bastian and Grosch, Benedikt and Lindner, Martin and Fuhrländer-Völker, Daniel and Strobel, Nina and Weigold, Matthias},
  date = {2022-01},
  journaltitle = {Energies},
  volume = {15},
