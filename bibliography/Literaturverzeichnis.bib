  number = {SAND-2020-13473},
  institution = {{Sandia National Lab. (SNL-CA), Livermore, CA (United States); Sandia National Lab. (SNL-NM), Albuquerque, NM (United States); Hobart and William Smith Colleges, Geneva, NY (United States)}},
